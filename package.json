{
  "name": "kanan-landing-pages",
  "version": "1.0.0",
  "description": "Kanan.co landing pages monorepo - IELTS, GRE, GMAT",
  "private": true,
  "workspaces": [
    "apps/*",
    "shared/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/ielts",
    "dev:ielts": "npm run dev --workspace=apps/ielts",
    "dev:gre": "npm run dev --workspace=apps/gre",
    "dev:gmat": "npm run dev --workspace=apps/gmat",
    "build": "npm run build --workspaces",
    "build:ielts": "npm run build --workspace=apps/ielts",
    "build:gre": "npm run build --workspace=apps/gre",
    "build:gmat": "npm run build --workspace=apps/gmat",
    "preview": "npm run preview --workspace=apps/ielts",
    "lint": "npm run lint --workspaces",
    "type-check": "npm run type-check --workspaces"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}